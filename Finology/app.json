        "foregroundImage": "./assets/mainLogo.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.addy5643.MyApp"
    },
    "web": {
      "favicon": "./assets/mainLogo.png"
    },
    "plugins": [
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "001e81bb-6978-47f9-abce-d0da7cac3e72"
      }
    }
  }
